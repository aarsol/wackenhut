        <!--   Faculty Appraisal  Views -->

        <record id="aar_faculty_appraisal_form" model="ir.ui.view">
            <field name="name">aar.appraisal.faculty.form</field>
            <field name="model">aar.appraisal.faculty</field>
            <field name="arch" type="xml">
                <form string="Faculty Appraisal">
                    <header>
                        <field name="state"
                               widget="statusbar"/>
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"></field>
                                <field name="employee_id"></field>
                                <field name="start_date"></field>
                                <field name="end_date"></field>
                                <field name="birthday"></field>
                                <field name="designation"></field>
                                <field name="h_index"></field>
                                <field name="focus_area"></field>
                                <field name="active"></field>

                            </group>
                            <group>
                                <field name="reporting_id"></field>
                                <field name="payscale"></field>
                                <field name="counter_sign_id"></field>
                                <field name="senior_counter_sign_id"></field>
                                <field name="joining_date"></field>
                                <field name="email"></field>
                                <field name="phone"></field>
                                <field name="qualification"></field>
                                <field name="specialization"></field>
                                <field name="citations"></field>
                                <!--                                <field name="grandTotalMarks"></field>-->
                                <!--                                <field name="totalObtainedMarks"></field>-->
                            </group>

                        </group>
                        <group>
                            <group>
                                <field name="aar_personal_trait_marks"></field>
                                <field name="aar_teaching_marks"></field>
                                <field name="aar_student_supervision_marks"></field>
                                <field name="aar_publication_marks"></field>

                            </group>
                            <group>
                                <field name="aar_funding_activity_marks"></field>
                                <field name="aar_institutional_marks"></field>
                                <field name="aar_total_marks"></field>
                                <field name="overall_grade"></field>
                            </group>
                        </group>
                        <group>
                            <field name="courses_taught_ids"></field>
                            <field name="student_supervision_ids"></field>
                            <field name="research_publication_ids"></field>
                            <field name="funding_activities_ids"></field>
                            <field name="institutional_services_ids"></field>
                        </group>
                        <group>
                            <field name="personal_trait_ids"></field>
                            <field name="aar_student_supervision_ids"></field>
                            <field name="publication_ids"></field>
                            <field name="aar_funding_activity_ids"></field>
                            <field name="aar_institutional_services_ids"></field>
                            <field name="aar_teaching_ids"></field>
                            <field name="signing_authority_remarks"></field>
                            <field name="signing_degree_of_contact"></field>
                            <field name="counter_sign_remarks"></field>
                            <field name="senior_counter_sign_remarks"></field>
                            <field name="senior_counter_sign_recommendation"></field>

                        </group>
                    </sheet>

                </form>
            </field>
        </record>

