        <record id="aar_appraisal_employee_form" model="ir.ui.view">
            <field name="name">aar.appraisal.employee.form</field>
            <field name="model">aar.appraisal.employee</field>
            <field name="arch" type="xml">
                <form string="Performance Evaluation">
                    <header>
                        <field name="state"
                               widget="statusbar"/>
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"></field>
                                <field name="employee_id"></field>
                                <field name="start_date"></field>
                                <field name="end_date"></field>
                                <field name="birthday"></field>
                                <field name="active"></field>

                            </group>
                            <group>
                                <field name="reporting_id"></field>
                                <field name="payscale"></field>
                                <field name="counter_sign_id"></field>
                                <field name="senior_counter_sign_id"></field>
                                <field name="senior_counter_sign_id_two"></field>
                                <field name="senior_counter_sign_id_three"></field>
                                <field name="joining_date"></field>
                                <field name="grandTotalMarks"></field>
                                <field name="totalObtainedMarks"></field>
                                <field name="percentage_marks"></field>
                                <field name="marks_grading"></field>
                            </group>
                        </group>
                        <label for="participation_ids">Participation in Conferences</label>
                        <field name="participation_ids"></field>

                        <label for="committee_membership_ids">Statuary Committees Employee was member of</label>
                        <field name="committee_membership_ids"></field>
                        <group>
                            <field name="post_description"></field>
                            <field name="additional_duties"></field>
                            <field name="achievements"></field>
                        </group>
                        <label for="section_ids">Performance Evaluation</label>
                        <field name="section_ids">
                                <tree editable="0" string="Sections">
                                    <field name="name"/>
                                    <field name="selected_name"/>
                                    <field name="selected_marks"/>
                                </tree>
                        </field>
                        <label for="reporting_observations">Observations of reporting officer</label>
                        <field name="reporting_observations"></field>
                        <label for="counter_sign_observations">Total Marks of Counter Sign Questions</label>
                        <field name="counter_sign_observations"></field>
                        <group>
                            <!--                            <label for="evaluation_quality">Quality of Evaluation</label>-->
                            <field name="evaluation_quality"></field>
                        </group>
                        <label for="senior_counter_sign_remarks">Senior Counter Sign Remarks</label>
                        <field name="senior_counter_sign_remarks"></field>
                        <label for="senior_counter_sign_recommendation">Senior Counter Sign Recommendation</label>
                        <field name="senior_counter_sign_recommendation"></field>


                    </sheet>
                </form>
            </field>
        </record>

