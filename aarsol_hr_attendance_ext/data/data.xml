<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="sequence_hr_month_attendance_variation" model="ir.sequence">
            <field name="name">HR Month Attendance Variation</field>
            <field name="code">hr.employee.month.attendance.variations</field>
            <field name="prefix">ATT/VAR/%(year)s%(month)s/</field>
            <field name="padding">5</field>
        </record>

        <record id="sequence_hr_employee_overtime" model="ir.sequence">
            <field name="name">HR Employee Overtime</field>
            <field name="code">hr.employee.overtime</field>
            <field name="prefix">OT/%(year)s%(month)s/</field>
            <field name="padding">5</field>
        </record>

        <record id="sequence_hr_employee_monthly_overtime_register" model="ir.sequence">
            <field name="name">HR Employee Monthly Overtime Register</field>
            <field name="code">hr.employee.monthly.overtime.register</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
        </record>

        <record id="sequence_hr_employee_monthly_overtime" model="ir.sequence">
            <field name="name">HR Employee Monthly Overtime</field>
            <field name="code">hr.employee.monthly.overtime</field>
            <field name="prefix">MON/OT/%(year)s%(month)s/</field>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>