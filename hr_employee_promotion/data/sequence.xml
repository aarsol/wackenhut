<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_hr_employee_promotion" model="ir.sequence">
            <field name="name">HR Employee Promotion</field>
            <field name="code">hr.employee.promotion</field>
            <field name="prefix">Prom-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_hr_employee_demotion" model="ir.sequence">
            <field name="name">HR Employee Demotion</field>
            <field name="code">hr.employee.demotion</field>
            <field name="prefix">Demo-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_hr_employee_transfer" model="ir.sequence">
            <field name="name">HR Employee Transfer</field>
            <field name="code">hr.employee.transfer</field>
            <field name="prefix">TRAN-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>